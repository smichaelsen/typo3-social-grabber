<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Feed Options</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <channel>
                        <TCEforms>
                            <label>Channel</label>
                            <config>
                                <type>select</type>
                                <foreign_table>tx_socialgrabber_channel</foreign_table>
                                <foreign_table_where>ORDER BY tx_socialgrabber_channel.title ASC</foreign_table_where>
                                <renderType>selectMultipleSideBySide</renderType>
                                <minitems>1</minitems>
                                <maxitems>99</maxitems>
                            </config>
                        </TCEforms>
                    </channel>
                    <limit>
                        <TCEforms>
                            <label>Limit number of posts</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </limit>
                    <filter_topics>
                        <TCEforms>
                            <label>Only output posts with the following topics/hashtags (comma separated)</label>
                            <config>
                                <type>input</type>
                            </config>
                        </TCEforms>
                    </filter_topics>
                        <exclude_shared_posts>
                        <TCEforms>
                            <label>Exclude shared posts (retweets) from output</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </exclude_shared_posts>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>